{
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": [2, "unix"]
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "globals": {
        "$": true,
        "__DEV__": true
    },
    "plugins": [
        "react"
    ],
    "extends": ["eslint:recommended", "plugin:react/recommended"]
}
