{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [{
        "taskName": "install",
        "args": ["install"]
    }, {
        "taskName": "update",
        "args": ["update"]
    }, {
        "taskName": "test",
        "args": ["run", "test"]
    }, {
        "taskName": "test:watch",
        "args": [
            "run", "test:watch"
        ]
    }, {
        "taskName": "build",
        "args": [
            "run", "build"
        ]
    }]
}